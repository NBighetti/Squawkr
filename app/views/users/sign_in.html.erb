<body id='welcomeBody'>
  <nav class='login'>
    <h1>Squawkr</h1>
    <div class="navLogin">
      <%= form_tag('/log_in', method: :post) do %>
        <%= label_tag :username %>
        <%= text_field_tag :username %>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        <%= submit_tag "Sign-In" %>
      <% end %>
    </div>
  </nav>
</body>
